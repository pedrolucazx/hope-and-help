    "lint-staged": "^15.2.10",
    "prettier": "^3.4.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1",
    "vite-tsconfig-paths": "^5.1.4"
